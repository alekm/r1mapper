  "components" : {
    "schemas" : {
      "AssignmentACXRequest" : {
        "required" : [ "deviceType", "quantity" ],
        "type" : "object",
        "properties" : {
          "deviceType" : {
            "type" : "string",
            "description" : "MSP entitlement device type.",
            "example" : "MSP_WIFI, MSP_SWITCH, MSP_APSW",
            "enum" : [ "MSP_WIFI", "MSP_SWITCH", "MSP_EDGE", "MSP_APSW", "MSP_SLTN_TOKEN" ]
          },
          "quantity" : {
            "type" : "integer",
            "description" : "Quantity of devices permitted by this assignment.",
            "format" : "int64",
            "example" : 15
          },
          "useTemporaryMspEntitlement" : {
            "type" : "boolean",
            "description" : "Trial MSP entitlement to be used for this assignment, if this parameter is not supplied in the request, it defaults to false.",
            "example" : false
          },
          "deviceSubType" : {
            "type" : "string"
          }
        },
        "description" : "List of assignments"
      },
      "CreateAssignmentRequest" : {
        "required" : [ "assignments", "endDate", "startDate" ],
        "type" : "object",
        "properties" : {
          "assignments" : {
            "type" : "array",
            "description" : "List of assignments",
            "items" : {
              "$ref" : "#/components/schemas/AssignmentACXRequest"
            }
          },
          "startDate" : {
            "type" : "string",
            "description" : "The date when the assignments starts.",
            "example" : "2020-09-14 01:12:51Z"
          },
          "endDate" : {
            "type" : "string",
            "description" : "The date when the assignments ends.",
            "example" : "2020-09-14 01:12:51Z"
          }
        }
      },
      "Assignment" : {
        "type" : "object",
        "properties" : {
          "id" : {
            "type" : "integer",
            "format" : "int64"
          },
          "mspTenantId" : {
            "type" : "string"
          },
          "mspEcTenantId" : {
            "type" : "string"
          },
          "deviceType" : {
            "type" : "string",
            "enum" : [ "MSP_WIFI", "MSP_SWITCH", "MSP_EDGE", "MSP_APSW", "MSP_SLTN_TOKEN" ]
          },
          "deviceSubType" : {
            "type" : "string"
          },
          "quantity" : {
            "type" : "integer",
            "format" : "int64"
          },
          "dateEffective" : {
            "type" : "string",
            "format" : "date-time"
          },
          "dateExpires" : {
            "type" : "string",
            "format" : "date-time"
          },
          "trialAssignment" : {
            "type" : "boolean"
          },
          "dateAssignmentCreated" : {
            "type" : "string",
            "format" : "date-time"
          },
          "dateAssignmentRevoked" : {
            "type" : "string",
            "format" : "date-time"
          },
          "createdBy" : {
            "type" : "string"
          },
          "revokedBy" : {
            "type" : "string"
          },
          "extendedTrial" : {
            "type" : "boolean"
          },
          "alm" : {
            "type" : "boolean"
          },
          "ownAssignment" : {
            "type" : "boolean"
          },
          "status" : {
            "type" : "string",
            "enum" : [ "FUTURE", "VALID", "EXPIRED", "REVOKED" ]
          }
        }
      },
      "AssignmentOperationResponse" : {
        "type" : "object",
        "properties" : {
          "success" : {
            "type" : "boolean"
          },
          "assignments" : {
            "type" : "array",
            "items" : {
              "$ref" : "#/components/schemas/Assignment"
            }
          },
          "errorBrief" : {
            "type" : "string",
            "enum" : [ "NO_CREATED_ASSIGNMENTS", "ASSIGNMENT_CREATION_BAD_INPUT", "NO_MSP_ENTITLEMENTS", "MSP_ENTITLEMENTS_NOT_FOUND", "INSUFFICIENT_MSP_ENTITLEMENTS", "ASSIGNMENT_NOT_FOUND", "TENANT_NOT_MSP", "TENANT_NOT_FOUND", "ENTITLEMENT_ID_NOT_FOUND", "ASSIGNMENT_NOT_REVOCABLE", "ASSIGNMENT_NOT_REPLACEABLE", "ASSIGNMENT_ALREADY_REVOKED", "ASSIGNMENT_IS_EXPIRED", "INTERNAL_SERVER_ERROR", "K2A_MIGRATION_MISSING_EXPORT_DATA", "K2A_MIGRATION_NO_ASSIGNMENT", "K2A_MIGRATION_EXCEPTION", "K2A_MIGRATION_NO_MSP_ENTITLEMENTS", "K2A_MIGRATION_INSUFFICIENT_MSP_ENTITLEMENTS", "LOCK_ACQUISITION_TIMEOUT", "INVALID_INPUT_PARAMETER", "NOT_ACCEPTABLE", "TRIAL_ASSIGNMENT_NOT_ALLOW", "TRIAL_ASSIGNMENT_UPDATE_NOT_ALLOW", "TRIAL_ASSIGNMENT_REVOKE_NOT_ALLOW", "ASSIGNMENT_NOT_REPLACED" ]
          },
          "errorMessage" : {
            "type" : "string"
          },
          "ve" : {
            "$ref" : "#/components/schemas/ValidationErrors"
          }
        }
      },
      "ValidationErrors" : {
        "type" : "object",
        "properties" : {
          "object" : {
            "type" : "string"
          },
          "objectName" : {
            "type" : "string"
          },
          "value" : {
            "type" : "string"
          },
          "expected" : {
            "type" : "string"
          }
        }
      },
      "ReplacementACXRequest" : {
        "required" : [ "assignmentId", "quantity" ],
        "type" : "object",
        "properties" : {
          "assignmentId" : {
            "type" : "integer",
            "description" : "The ID of the assignment to be replaced.",
            "format" : "int64",
            "example" : 231
          },
          "quantity" : {
            "type" : "integer",
            "description" : "Quantity of devices permitted by this replacement assignment; the replacement assignment will be for the same MSP-EC and have the same device type, device sub type and is trial values as the assignment being revoked.",
            "format" : "int64",
            "example" : 10
          },
          "startDate" : {
            "type" : "string",
            "description" : "The date when the assignment starts.",
            "example" : "2019-09-14 01:12:51Z"
          },
          "endDate" : {
            "type" : "string",
            "description" : "The date when the assignment ends.",
            "example" : "2020-09-14 01:12:51Z"
          }
        }
      },
      "MspEntitlement" : {
        "type" : "object",
        "properties" : {
          "id" : {
            "type" : "string"
          },
          "deviceType" : {
            "type" : "string",
            "enum" : [ "MSP_WIFI", "MSP_SWITCH", "MSP_EDGE", "MSP_APSW", "MSP_SLTN_TOKEN" ]
          },
          "deviceSubType" : {
            "type" : "string"
          },
          "sku" : {
            "type" : "string"
          },
          "quantity" : {
            "type" : "integer",
            "format" : "int64"
          },
          "effectiveDate" : {
            "type" : "string",
            "format" : "date-time"
          },
          "expirationDate" : {
            "type" : "string",
            "format" : "date-time"
          },
          "lastNotificationDate" : {
            "type" : "string",
            "format" : "date-time"
          },
          "alm" : {
            "type" : "boolean"
          },
          "almActivationId" : {
            "type" : "string"
          },
          "almSoEntQuantity" : {
            "type" : "integer",
            "format" : "int64"
          },
          "isR1Sku" : {
            "type" : "string"
          },
          "status" : {
            "type" : "string",
            "enum" : [ "FUTURE", "VALID", "EXPIRED" ]
          },
          "r1Sku" : {
            "type" : "boolean"
          },
          "isTrial" : {
            "type" : "boolean"
          }
        }
      },
      "AlmExtraEntitlementInfoDto" : {
        "type" : "object",
        "properties" : {
